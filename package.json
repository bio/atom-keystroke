{
  "name": "keystroke",
  "main": "./lib/main",
  "version": "0.2.1",
  "description": "define command from keystroke",
  "keywords": [],
  "repository": "https://github.com/t9md/atom-keystroke",
  "license": "MIT",
  "engines": {
    "atom": "^1.19.0"
  },
  "configSchema": {
    "commands": {
      "type": "array",
      "default": [],
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "keystroke": {
            "type": "string"
          },
          "scope": {
            "type": "string"
          }
        }
      }
    }
  }
}
